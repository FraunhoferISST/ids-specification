        }, {
            "@type": [ "dcat:DataService", "ids:TRANSFER_SERVICE" ],
            "dcat:endpointURL": {
                "@type": "xsd:anyURI",
                "@value": "https://provider.com/connector/transfer"
            }
        } ]
    }, {
        "@type": "dcat:Distribution",
