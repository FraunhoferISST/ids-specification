                "odrl:rightOperand": "idsc:TRUST_SECURITY_PROFILE"
            } ]
        } ],
        "odrl:prohibition": [ {
            "odrl:action": {"@id": "odrl:distribute"}
        } ]
    },
    "dcat:distribution": [ {
        "@type": "dcat:Distribution",
        "dct:title" : "Distribution#1",
